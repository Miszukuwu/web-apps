@model List<MyStudent>
<h1>Lista Studentów</h1>
<table class="table table-striped">
    <thead>
    <tr>
        <th>Lp.</th>
        <th>Imię</th>
        <th>Nazwisko</th>
        <th>Wiek</th>
    </tr>
    </thead>
    @{ int i = 0;}
    @foreach (MyStudent student in Model) {
        i++;
        <tr>
            <td>@i</td>
            <td>@student.FirstName</td>
            <td>@student.LastName</td>
            <td>@student.Age</td>
            <td><a asp-action="Delete" asp-route-id="@student.Id" class="btn btn-danger">Usuń</a></td>
        </tr>
    }
</table>